@inproceedings{rodriguezAutomaticDetectionRolling2022b,
  title = {Automatic {{Detection}} of {{Rolling Element Bearing Faults}} to {{Be Applied}} on {{Mechanical Systems Comprised}} by {{Gears}}},
  booktitle = {Nonstationary {{Systems}}: {{Theory}} and {{Applications}}},
  author = {Rodr{\'i}guez, Andy and Hern{\'a}ndez, Fidel and Ruiz, Mario},
  editor = {Chaari, Fakher and Leskow, Jacek and Wylomanska, Agnieszka and Zimroz, Radoslaw and Napolitano, Antonio},
  year = {2022},
  series = {Applied {{Condition Monitoring}}},
  pages = {217--234},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-030-82110-4_12},
  abstract = {This research aims to develop and validate a method for tacho-less automatic detection of faults in rolling bearings for mechanical systems comprised by gears. The proposed method was based on the application of some mode decomposition technique in order to extract monocomponent signals from the vibration and to calculate an indicator of the modulation produced by the rolling element bearing fault. The computation of this indicator was performed by means of Lock-in Amplifiers, which are used in order to extract, through a synchronous approach, spectral components from non-stationary signals. A novel algorithm, previously applied on gear fault detection, was adapted and used in order to estimate the rotational speed. The effectiveness of the method was assessed through experiments with real signals. Besides, the capability of the indicator for serving as a relative measure of the fault severity was verified.},
  isbn = {978-3-030-82110-4},
  langid = {english},
  keywords = {Fault detection,Gear vibration,Lock-in Amplifiers,Modulation detection,Rolling bearing fault,Singular Spectrum Decomposition},
  file = {C\:\\Users\\andyrodlor\\Zotero\\storage\\N4FII398\\Rodríguez et al. - 2022 - Automatic Detection of Rolling Element Bearing Fau.pdf}
}

@misc{rodriguezlorenzoCityPathTomography2023,
  title = {City Path Tomography: Reconstructing Square Road Network from Artificial Users Mobile Phone Data.},
  shorttitle = {City Path Tomography},
  author = {Rodriguez Lorenzo, Andy and {Lage-Castellanos}, Alejandro},
  year = {2023-03-01. Accepted in Cuban Journal of Physics}, 
  number = {arXiv:2303.00840},
  eprint = {2303.00840},
  publisher = {{arXiv}},
  doi = {10.48550/arXiv.2303.00840},
  urldate = {2023-08-10},
  abstract = {Population mobility can be studied readily and cheaply using cellphone data, since people's mobility can be approximately mapped into tower-mobile registries. We model people moving in a grid-like city, where edges of the grid are weighted and paths are chosen according to overall weights between origin and destination. Cellphone users leave sparse signals in random nodes of the grid as they move by, mimicking the type of data collected from the tower-cellphone interactions. From this noisy data we seek to build a model of the city, \{\textbackslash it i.e.\} to predict probabilities of paths from origin to destination. We focus on the simplest case where users move along shortest paths (no loops, no going backwards). In this simplified setting, we are able to infer the underlying weights of the edges (akin to road transitability) with an inverse statistical mechanic model.},
  archiveprefix = {arxiv},
  keywords = {Condensed Matter - Statistical Mechanics,Physics - Physics and Society},
  annotation = {Accepted in Cuban Journal of Physics},
  file = {C\:\\Users\\andyrodlor\\Zotero\\storage\\MKD89G78\\Lorenzo y Lage-Castellanos - 2023 - City path tomography reconstructing square road n.pdf;C\:\\Users\\andyrodlor\\Zotero\\storage\\G5DS7EIJ\\2303.html}
}

@inproceedings{rodriguezlorenzoTrueHarmonicIdentificationbased2022,
  title = {True Harmonic Identification-Based Monocomponent Signal Selection for the Instantaneous Rotational Speed Estimation Performed by {{TARSE}}},
  booktitle = {2022 {{International Conference}} on {{Electrical}}, {{Computer}}, {{Communications}} and {{Mechatronics Engineering}} ({{ICECCME}})},
  author = {Rodr{\'i}guez Lorenzo, Andy and Hern{\'a}ndez Montero, Fidel E. and Ruiz Barrios, Mario L. and G{\'o}mez Mancilla, Julio C{\'e}sar},
  year = {2022},
  month = nov,
  pages = {1--7},
  doi = {10.1109/ICECCME55909.2022.9987905},
  abstract = {For the rotational speed estimation through gear vibration signals TARSE algorithm has been proposed. One of the steps of TARSE algorithm aims to identify the vibration produced by a gear and its harmonic order from a set of mono-component signals. The current procedure proposed for this task may lead to inaccurate results. This work proposes a procedure for the accurate identification of mono-component signals that include the modulations produced by gear vibration. The algorithm is based on a true harmonic identification procedure including the measure of interdependence relations among the set of mono-component signals, which is a novelty with respect to the original TARSE algorithm, and in an adequate computation of the Modulation Index, this allows to correctly identify the mono-component signal. Real vibration signals are used to validate the proposed technique. The results prove that the proposed algorithm could become an improved second stage of the TARSE method.},
  keywords = {Estimation,gear vibration,Gears,Harmonic analysis,Mechatronics,Modulation,modulation index,mono-component signal,TARSE algorithm,true harmonic identification,Velocity control,Vibrations},
  file = {C\:\\Users\\andyrodlor\\Zotero\\storage\\RDALQHAQ\\Lorenzo et al. - 2022 - True harmonic identification-based monocomponent s.pdf;C\:\\Users\\andyrodlor\\Zotero\\storage\\4JLYU5C9\\9987905.html}
}

@inproceedings{valdesAlgorithmDetectionFaults2023,
  title = {Algorithm for the Detection of Faults in Rolling Element Bearings Running under Tacholess and Variable Rotating Speed Conditions},
  booktitle = {Surveillance, {{Vibrations}}, {{Shock}} and {{Noise}}},
  author = {Vald{\'e}s, Lisa Mar{\'i}a and Ernesto Hern{\'a}ndez Montero, Fidel Ernesto and Rodr{\'i}guez Lorenzo, Andy},
  year = {2023},
  month = jul,
  publisher = {{Institut Sup\'erieur de l'A\'eronautique et de l'Espace [ISAE-SUPAERO]}},
  address = {{Toulouse, France}},
  urldate = {2023-09-04},
  abstract = {The goal of this research was to implement a new algorithm for the automatic detection of faults in rolling element bearings, in such a way that it does not depend on the estimation of the shaft rotational speed. The idea was that the algorithm could be applied under variable and unknown rotational speed conditions. The proposed algorithm was based on the detection of phase-relationships between spectral components that emerge when an amplitude modulation appears in the gathered vibration. To do that, a mode decomposition procedure, as well as the Hilbert transform, was applied in order to estimate a detection coefficient, which served as indicator of the presence of the modulation produced by the faults. Lock-in amplifiers were used in order to calculate such an indicator. The effectiveness of the method was validated through experiments preformed by using simulation and real signals. It was proven that the application of the proposed method can lead to an effective detection of the modulation featuring the existence of rolling element bearing faults.},
  file = {C\:\\Users\\andyrodlor\\Zotero\\storage\\MH9EYTB3\\Valdés et al. - 2023 - Algorithm for the detection of faults in rolling e.pdf}
}
